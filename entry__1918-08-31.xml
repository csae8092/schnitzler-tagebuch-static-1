<tei:fw xmlns:tei="http://www.tei-c.org/ns/1.0" type="header"><fw place="top_center"><date type="range">1918: VIII 31 - IX 5</date></fw> <fw n="175" place="top_right" type="pageNum">175</fw></tei:fw>
